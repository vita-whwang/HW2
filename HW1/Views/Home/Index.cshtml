@model IEnumerable<HW1.Models.客戶資訊>

@{
    ViewBag.Title = "Index";
}

<h2>報表</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.客戶名稱)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.客戶聯絡人數量)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.客戶銀行資訊數量)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.客戶名稱)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.客戶聯絡人數量)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.客戶銀行資訊數量)
        </td>
    </tr>
}

</table>
